{
  "name": "compass",
  "version": "0.0.1",
  "description": "The Plaid Integration",
  "author": "Mobilefirst",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "20.19.0",
    "npm": "10.9.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prebuild": "rimraf dist",
    "prisma:seed": "ts-node prisma/seed.ts",
    "migrate": "prisma migrate deploy && npm run prisma:seed",
    "prisma:template": "ts-node prisma/template.seed.ts"
  },
  "dependencies": {
    "@angular/common": "^19.2.7",
    "@angular/core": "^19.2.7",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.20",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.0.20",
    "@nestjs/schedule": "^6.0.0",
    "@prisma/client": "^6.10.1",
    "@types/multer": "^1.4.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "crypto-ts": "^1.0.2",
    "dotenv": "^16.4.5",
    "node-forge": "^1.3.1",
    "nodemailer": "^7.0.5",
    "pdf-lib": "^1.17.1",
    "openai": "^4.67.3",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.1",
    "uuid": "^11.1.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.0.20",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.14.1",
    "@types/node-forge": "^1.3.11",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "axios": "^1.10.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "prisma": "^6.10.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "main": "main.ts",
  "repository": "https://github.com/sultankhanmf/compass.git"
}
